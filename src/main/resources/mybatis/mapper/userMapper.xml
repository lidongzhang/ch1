<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="app.mybatis.dao.MyBatisUserDao">
    <select id="getUser" resultType="app.mybatis.entity.User" >
            select id, name as userName, note from user where id= 3
    </select>
</mapper>